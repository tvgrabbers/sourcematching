					{"tag":"li", "index":{"link":2},"type":"lower-ascii","member-off":"channelid"}],
				[{"path":4},{"tag":"h6", "attrs":{"class":"title"}, "node": 5}],
				[{"path":5},{"tag":"span", "attrs":{"class":"broadcaster"}, "lstrip":"(", "rstrip":")"}],
				[{"attr":"data-description"}],
				[{"attr":"style","split":[[":",-1]],"rstrip":"px;","multiplier":60,"divider":7, "type":"timedelta"}],
